<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Tabela</title>
  </head>
  <body>
    <Table>
      <thead>
        <tr>
          <% header.forEach(col=>{ %>
          <th><%= col %></th>
          <% }); %>
        </tr>
      </thead>

      <Tbody>
        <% rows.forEach(row =>{ %>
        <tr>
          <% row.forEach(element=>{ %>
          <td><%= element %></td>
          <% }) %>
        </tr>
        <% }); %>
      </Tbody>
    </Table>
  </body>
</html>
